TB_URL=https://your-thingsboard-host
TB_TOKEN=your_access_token

# Asset / Device IDs
TB_PLNT_ID=asset_uuid_for_generation
TB_WSTN_ID=device_uuid_for_weather_station

# Comma-separated telemetry keys
TB_GEN_KEYS=energymeter_dailygeneration
TB_IRR_KEYS=wstn1_horiz_irr,wstn1_tilted_irr
TB_INV_KEYS=active_power,current_a,current_b,current_c

# Inverters: comma separated, format <name>:<device_id>
TB_INVERTERS=B1-04:uuid1,B1-08:uuid2,B1-13:uuid3,B1-17:uuid4,B2-04:uuid5,B2-08:uuid6,B2-13:uuid7,B2-17:uuid8

# Auth helper only
TB_USERNAME=your_login_email
TB_PASSWORD=your_login_password

# Optional overrides
TB_TZ_OFFSET=+05:30
TB_START_DATE=2025-01-01
TB_OUTPUT_DIR=data
TB_REQUEST_TIMEOUT_S=30
TB_GEN_MAX_J=360000000000
